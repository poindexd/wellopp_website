section#stats(style='position:absolute;bottom: 50px; width: 100%')
	.container
		.row.hide-on-med-and-down
			.col.m3.s6: .card.center
				.chart
					.percentage(data-percent='10000' data-bar-color='transparent' data-track-color='transparent')
						span 10000
						sup +
					p Patient Engagements

			.col.m3.s6: .card.center
				.chart
					.percentage(data-percent='85', data-bar-color='transparent')
						span 85
						sup %
					p Patient Satisfaction

			.col.m3.s6: .card.center
				.chart
					.percentage(data-percent='29', data-bar-color='transparent' data-track-color='transparent')
						span 29
					p Minutes Saved

			.col.m3.s6: .card.center
				.chart
					.percentage(data-percent='47' data-bar-color='transparent')
						span 47
						sup %
					p Readmission Reduction

		.row.hide-on-large-only(style='position: relative; bottom: -40px')
			.col.s12: .card.center
				span 10,227 Surveys Taken
				.progress.transparent
			.col.s12: .card.center
				span 97% Patient Satisfaction
				.progress.transparent
					.determinate.green(style='width:97%')
			.col.s12: .card.center
				span 75% Kittens Saved
				.progress.transparent
					.determinate.orange(style='width:75%')
			.col.s12: .card.center
				span 46% Readmissions Reduced
				.progress.transparent
					.determinate.red(style='width:46%')